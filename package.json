{
  "name": "dlpipe-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.4",
    "immutable": "^3.8.2",
    "moment": "^2.22.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-parallax": "^2.0.1",
    "react-redux": "^5.1.0",
    "react-redux-loading-bar": "^4.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "rmwc": "^3.0.9"
  },
  "devDependencies": {
    "node-sass": "^4.9.4",
    "react-scripts": "2.1.1",
    "npm-run-all": "^4.1.3",
    "react-dev-utils": "6.1.1"
  },
  "scripts": {
    "build-css": "node-sass --include-path node_modules --include-path src src/index.scss public/bundle.css --output-style compressed --source-map false",
    "watch-css": "npm run build-css && node-sass --include-path node_modules --include-path src src/index.scss public/bundle.css -w -r --source-map true",
    "start-js": "react-scripts start",
    "start": "npm-run-all --parallel start-js watch-css",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
